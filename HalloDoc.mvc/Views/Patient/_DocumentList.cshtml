@using DataAccess.Models;
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

@model IList<Requestwisefile>;

<div class="table-responsive">
    <table class="table dataTable table-hover" id="data-table" aria-describedby="data-table_info">
        <thead>
            <tr>
                <th class="sorting" data-value="Description" style="width:70%">
                    File
                </th>
               
            </tr>
        </thead>
        <tbody>
            @if (Model != null && Model.Count() > 0)
            {
                @foreach (Requestwisefile caseStatus in Model)
                {
                    <tr>
                        <td>@caseStatus.Filename</td>
                        
                    </tr>
                }
            }
            else
            {
                <tr>
                    <td colspan="3" style="text-align: center;">No records found.</td>
                </tr>
            }
        </tbody>
    </table>
</div>

